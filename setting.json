{
    "workbench.colorTheme": "Catppuccin Mocha",
    "windsurf.autocompleteSpeed": "fast",
    "tabnine.experimentalAutoImports": true,
    "windsurf.cascadeOpenOnReload": false,
    "editor.fontFamily": "Cascadia Code",
    "editor.wordWrap": "on",
    "editor.guides.bracketPairs": "active",
    "editor.tokenColorCustomizations": {
    "textMateRules": [
        {
            "scope": [
                "comment",
                "Settings",
                "SettingsHeading",
                "SettingsEntityName",
                "comment.block",
                "comment.block.documentation",
                "comment.line",
                "keyword",
                "keyword.control",
                "keyword.operator",
                "keyword.operator.new",
                "keyword.operator.expression",
                "constant",
                "constant.language",
                "constant.numeric",
                "constant.numeric.integer",
                "constant.numeric.float",
                "storage",
                "storage.type",
                "storage.type.function",
                "storage.modifier",
                "storage.modifier.async",
                "storage.modifier.export",
                "storage.modifier.import",
                "storage.modifier.package",
                "storage.modifier.package-name",
                "variable",
                "variable.language",
                "variable.parameter",
                "variable.other.readwrite",
                "variable.other.readwrite.alias",
                "constant.character",
                "string.quoted",
                "string.quoted.single",
                "string.quoted.double",
                "string.quoted.triple",
                "string.quoted.other",
                "string.unquoted",
                "dictionaries.key",
                "dictionaries.value",
                "support",
                "support.function",
                "support.constant",
            ],
        "settings": {
            "fontStyle": "italic",
        }
        }
    ]
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "git.enableSmartCommit": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.preferredHighContrastColorTheme": "Monokai",
    "terminal.integrated.tabs.defaultIcon": "history",
    "workbench.activityBar.iconClickBehavior": "focus",
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "workbench.iconTheme": "material-icon-theme",
    "git.autofetch": true,
    "editor.matchBrackets": "near",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.terraform/**": true,
        "**/.terraform.lock.hcl": true,
        "**/.terraform.tfstate": true
    },
    "git.confirmSync": false,
    "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font",
    "editor.inlineSuggest.enabled": true,
    "editor.cursorSurroundingLinesStyle": "all",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "expand",
    "editor.cursorStyle": "line",
    "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss19', 'ss20'",
    "workbench.sideBar.location": "right",
    "python.createEnvironment.trigger": "off",
    "catppuccin.accentColor": "pink",
    "editor.tabCompletion": "on",
}
