{
    "editor.fontFamily": "\"Cascadia Code\", \"Cartograph CF\"",
    "[ansible-jinja]": {
        "editor.formatOnSave": true
    },
    "editor.cursorBlinking": "expand",
    "editor.cursorStyle": "line",
    "editor.fontLigatures": "'calt', 'ss01'",
    "github.copilot.advanced": {
        "enabled": true
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "python": true,
        "javascript": true,
        "typescript": true,
        "go": true,
        "ruby": true,
        "php": true,
        "java": true,
        "csharp": true,
        "cpp": true,
        "yaml": true,
        "json": true
    },
    "editor.cursorWidth": 2,
    "editor.cursorSurroundingLines": 10,
    "editor.inlineSuggest.enabled": true,
    "editor.cursorSurroundingLinesStyle": "all",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "Settings",
                    "SettingsHeading",
                    "SettingsEntityName",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line",
                    "punctuation.definition.comment",
                    "entity.name.type.class",
                    "entity.other.attribute-name",
                    "entity.other.attribute-name.id",
                    "entity.other.attribute-name.class",
                    "entity.other.attribute-name.pseudo-class",
                    "keyword",
                    "keyword.control",
                    "keyword.operator",
                    "keyword.operator.new",
                    "keyword.operator.expression",
                    "constant",
                    "constant.language",
                    "constant.numeric",
                    "constant.numeric.integer",
                    "constant.numeric.float",
                    "storage",
                    "storage.type",
                    "storage.type.function",
                    "storage.modifier",
                    "storage.modifier.async",
                    "storage.modifier.export",
                    "storage.modifier.import",
                    "storage.modifier.package",
                    "storage.modifier.package-name",
                    "variable",
                    "variable.language",
                    "variable.parameter",
                    "variable.other",
                    "variable.other.readwrite",
                    "variable.other.readwrite.alias",
                    "constant.character",
                    "constant.character.escape",
                    "constant.character.escape.backslash",
                    "constant.character.escape.delimiter",
                    "constant.character.escape.delimiter.begin",
                    "constant.character.escape.delimiter.end",
                    "string.quoted",
                    "string.quoted.single",
                    "string.quoted.double",
                    "string.quoted.triple",
                    "string.quoted.other",
                    "string.unquoted",
                    "dictionaries.key",
                    "dictionaries.value",
                    "support",
                    "support.function",
                    "support.constant",
                ],
                "settings": {
                    "fontStyle": "italic",
                }
            }, 
                
            ]
    },
        
    
    "workbench.startupEditor": "none",
    "redhat.telemetry.enabled": true,
    "remote.SSH.remotePlatform": {
        "master": "linux",
        "192.168.1.83": "linux",
        "192.168.1.87": "linux",
        "192.168.1.35": "linux",
        "default": "linux",
        "app": "linux",
        "Jenkins": "linux",
        "jenkins": "linux",
        "Slave": "linux",
        "slave1": "linux"
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "git.enableSmartCommit": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.preferredHighContrastColorTheme": "Monokai",
    "terminal.integrated.tabs.defaultIcon": "history",
    "workbench.activityBar.iconClickBehavior": "focus",
    "aws.suppressPrompts": {
        "codeWhispererNewWelcomeMessage": true
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": true,

    "window.zoomLevel": 1,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "workbench.iconTheme": "material-icon-theme",
    "git.autofetch": true,
    "editor.guides.bracketPairs": "active",
    "editor.matchBrackets": "near",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true
    },
    "vs-kubernetes": {
        "disable-linters": ["resource-limits"],
},
"git.confirmSync": false,
"workbench.colorTheme": "One Dark Pro Monokai Darker"
}
